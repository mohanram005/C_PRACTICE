// Input: 1 2 4 5
// Output: 3



#include <stdio.h>

int main() {
    
    int arr[] = {1,2,4,5};
    int n = sizeof(arr)/sizeof(arr[0]);
    int last = arr[n - 1];
    int pos = 0;
    //printf("%d",last);
    
    for(int i = 1;i < last+1;i++){
        
        if(i == arr[pos]){ // if any thing dont match not increment the pos than check for another 
            pos++;
            
        }else{
             printf("%d",i);
        }
        
    }
    
    
    

    return 0;
}

//xor type 
#include <stdio.h>

int main() {
    int arr[] = {1, 2, 4, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    int N = arr[n - 1];  // Last element is 5

    int xor1 = 0;
    int xor2 = 0;

    // XOR 1 to N
    for (int i = 1; i <= N; i++) {
        xor1 ^= i;
        printf("the xor1=%d\n",xor1);
    }

    // XOR array elements
    for (int i = 0; i < n; i++) {
        xor2 ^= arr[i];
        printf("the xor2=%d\n",xor2);
    }

    int missing = xor1 ^ xor2;
    printf("Missing number = %d\n", missing);

    return 0;
}


//SUM METHOD
#include <stdio.h>

int main() {
    int arr[] = {1, 2, 4, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    int N = arr[n - 1];  // Last element is 5

    int total_sum = N *(N + 1)/2;
    printf("total_value = %d\n",total_sum);
    int actual_value = 0;
    //actual
    for(int i = 0;i < n;i++){
        actual_value += arr[i];
    }
    
    int missing = total_sum - actual_value;
    printf("%d",missing);
    return 0;
}

